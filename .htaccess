ErrorDocument 404 https://fnsurvival.github.io/404

<ifModule mod_rewrite.c>

#turn on .htaccess config
RewriteEngine On 

#Hide all php extentions
RewriteCond %{REQUEST_FILENAME} !-d 

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^(.*)$ $1.php [NC, L]

RewriteRule ^(.*)$ $1.html [NC, L]

Redirect 301 /ViewMore /https://fnsurvival.github.io/maintenance
Redirect 301 /Video /https://fnsurvival.github.io/maintenance

</ifModule>
