ErrorDocument 404 https://fnsurvival.github.io/404

<ifModule mod_rewrite.c>

#turn on .htaccess config
RewriteEngine On 

#Hide all php extentions
RewriteCond %{REQUEST_FILENAME} !-d 

RewriteCond %{REQUEST_FILENAME}\.php -f

RewriteRule ^(.*)$ $1.php [NC, L]

RewriteRule ^(.*)$ $1.html [NC, L]

</ifModule>

<IfModule mod_alias.c>

    
    RedirectMatch "^/o/Video.html\.html/?$" "https://fnsurvival.github.io/maintenance"
    RedirectMatch "^/ViewMore.html\.html/?$" "https://fnsurvival.github.io/maintenance"

    RedirectMatch "^/o/Video\.html/?$" "https://fnsurvival.github.io/maintenance"
    RedirectMatch "^/ViewMore\.html/?$" "https://fnsurvival.github.io/maintenance"

</IfModule>

<IfModule mod_mime.c>

    AddDefaultCharset utf-8

</IfModule>